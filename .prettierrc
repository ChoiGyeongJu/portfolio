{
    "arrowParens": "avoid",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "jsxSingleQuote": false,
    "printWidth": 100,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": [
      "^react(.*)",
      "^@mui/(.*)",
      "^@(.*)",
      "<THIRD_PARTY_MODULES>",
      "\\$routes",
      "\\$routes/(.*)",
      "\\$utils",
      "\\$utils/(.*)",
      "./(.*)",
      "\\$styles",
      "\\$styles/(.*)",
      "(.*).css"
    ],
    "importOrderSeparation": true
  }
  